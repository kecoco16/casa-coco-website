{
  "name": "casa-coco-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "clean": "rm -rf node_modules && yarn && yarn dev",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(ts|tsx|json)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --fix --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "validate": "npm-run-all --parallel check-format check-types lint build",
    "prepare": "husky install",
    "pre-commit-lint": "yarn validate && yarn lint-staged"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@react-google-maps/api": "^2.18.1",
    "cloudinary": "^1.35.0",
    "framer-motion": "^10.6.0",
    "i18next": "^22.4.11",
    "imagemin": "^8.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "next": "13.2.4",
    "next-i18next": "^13.2.2",
    "next-seo": "^5.15.0",
    "next-sitemap": "^4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-swipeable": "^7.0.0",
    "react-use-keypress": "^1.3.1"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.12",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5"
  }
}
